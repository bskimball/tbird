  "homepage": "",
  "private": true,
  "main": "server/src",
  "keywords": [
    "feathers"
  ],
  "type": "module",
  "author": {
    "name": "bskimball",
    "email": "brian.kimball@bdkonline.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server/src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run compile && yarn run mocha",
    "lint": "eslint server/src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev:old": "ts-node-dev --no-notify server/src/ & svelte-kit dev",
    "dev": "ts-node-dev -r esm server/src/ & svelte-kit dev",
    "start": "yarn run compile && yarn run build:client && node --experimental-specifier-resolution=node build/",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf build/ && tsc -p tsconfig.json",
    "build:client": "shx rm -rf public/* & svelte-kit build",
    "build": "yarn run compile && yarn run build:client"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
