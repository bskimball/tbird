    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.15",
    "@tailwindcss/forms": "^0.3.3",
    "compression": "^1.7.4",
